{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": [
        "node_modules/*"
      ],
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./src/renderer/assets/*"
      ],
      "@routes": [
        "./src/renderer/routes/*"
      ],
      "@page": [
        "./src/renderer/page/*"
      ],
      "@component": [
        "./src/renderer/component/*"
      ],
      "@store": [
        "./src/renderer/store/*"
      ],
    },
    "jsx": "react-jsx"
  },
  "include": [
    "src/**/*"
  ]
}