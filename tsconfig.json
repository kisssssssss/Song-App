{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "paths": {
      "*": [
        "node_modules/*"
      ],
      "@/*": [
        "./*"
      ],
      "assets/*": [
        "./src/renderer/assets/*"
      ],
      "routes/*": [
        "./src/renderer/routes/*"
      ],
      "page/*": [
        "./src/renderer/page/*"
      ],
      "components/*": [
        "./src/renderer/components/*"
      ],
      "store/*": [
        "./src/renderer/store/*"
      ],
      "service/*": [
        "./src/renderer/service/*"
      ],
      "utils/*": [
        "./src/renderer/utils/*"
      ],
      "types": [
        "./src/renderer/@types/*"
      ]
    },
    "jsx": "react-jsx"
  },
  "include": [
    "src/**/*"
  ]
}